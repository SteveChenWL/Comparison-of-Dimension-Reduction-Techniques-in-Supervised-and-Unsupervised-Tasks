   "source": [
    "from threading import Thread\n",
    "import pandas as pd\n",
    "import wikipedia\n",
    "import wikipediaapi\n",
    "import wptools\n",
    "import warnings\n",
    "#import multiprocessing as mp\n",
    "\n",
    "warnings.filterwarnings(\"ignore\")\n",
    "wiki_wiki = wikipediaapi.Wikipedia('en')\n",
    "book = pd.read_csv('book_names.csv')\n",
    "books = book['name'][:50]\n",
    "\n",
    "#print(\"Number of processors: \", mp.cpu_count())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 110,
   "metadata": {},
   "outputs": [],
   "source": [
    "# define functions to get data from wikipedia api\n",
    "# Reference: https://nbviewer.jupyter.org/gist/minrk/5732094\n"
    "\n",
    "def get_sections(bookname, level=0):\n",
    "    p = wiki_wiki.page(bookname)\n",
    "    sections = p.sections\n",
    "    titles = []\n",
    "    texts = []\n",
    "    for s in sections:\n",
    "        titles.append(s.title)\n",
    "        texts.append(s.text)\n",
    "    return dict(zip(titles, texts))  \n",
    "\n",
    "def get_content(book):\n",
    "    try:\n",
    "        page = wikipedia.page(book)\n",
    "        title.append(page.title)\n",
    "        return get_sections(book)   \n",
    "    except:\n",
    "        pass\n",
    "    \n",
    "def get_wikipedia(books,book_dict = None):\n",
    "    #process a number of ids, storing the results in a dict\n",
    "    if book_dict is None:\n",
    "        book_dict = {}\n",
    "    for b in books:\n",
    "        book_dict[b] = get_content(b)  \n",
    "    return book_dict\n",
    "    \n",
    "# thread process\n",
    "def threaded_process(books,nthreads):\n",
    "    #process the id range in a specified number of threads\n",
    "    book_dict = {}\n",
    "    threads = []\n",
    "    id_range = (range(len(books))) \n",
    "    # create the threads\n",
    "    for i in range(nthreads):\n",
    "        b = books[i::nthreads]\n",
    "        t = Thread(target = get_wikipedia,args =(b,book_dict))\n",
    "        threads.append(t)\n",
    "    \n",
    "    # start the threads\n",
    "    [ t.start() for t in threads]\n",
    "    # wait for the threads to finish\n",
    "    [ t.join() for t in threads]\n",
    "    return book_dict\n"
   ]
  },
